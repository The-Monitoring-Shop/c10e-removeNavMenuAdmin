{
  "manifest_version": 3,
  "name": "Chronosphere - Remove Navigation Menu's GoToAdmin",
  "version": "0.0.1",
  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
        "matches": ["https://university.chronosphere.io/*"],
        "js": ["removeNavMenu.js"],
        "all_frames": true,
        "run_at": "document_start"
    }
  ]
}